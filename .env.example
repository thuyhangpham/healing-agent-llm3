# ETL Sentiment System Environment Configuration
# Copy this file to .env and update with your local settings

# Application Settings
APP_NAME=ETL Sentiment
APP_VERSION=0.1.0
DEBUG=false

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3
OLLAMA_TIMEOUT=30

# LangGraph Configuration
LANGGRAPH_TIMEOUT=60
LANGGRAPH_MAX_RETRIES=3

# Selenium Configuration
SELENIUM_DRIVER_PATH=
SELENIUM_HEADLESS=true
SELENIUM_TIMEOUT=30

# Healing Configuration
HEALING_ENABLED=true
HEALING_MAX_ATTEMPTS=3
HEALING_BACKUP_ENABLED=true

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=data/logs/system.log

# Data Configuration
DATA_DIR=data
METRICS_FILE=data/metrics/healing_metrics.json